<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.fyp.hp.realestate.ShowCommission"
     android:background="@drawable/plain">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:layout_above="@+id/etUserName"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:maxHeight="5px"
        android:maxWidth="3px"
        app:srcCompat="@drawable/realestate"
        android:background="@drawable/realestatebackground"
        tools:ignore="ContentDescription,ObsoleteLayoutParam,PxUsage"
        />

    <Button
        android:id="@+id/btnGetCommision"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="89dp"
        android:text="Comission List" />

    <TextView
        android:id="@+id/txtViewCommList"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/btnGetCommision"
        android:layout_marginTop="17dp"
        android:text="List of Comission Appear here.."
        android:textAlignment="center"
        android:textAppearance="?android:textAppearanceLarge" />


</RelativeLayout>
